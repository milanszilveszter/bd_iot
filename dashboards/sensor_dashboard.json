      },
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"iot_sensors\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"environment\" and r._field == \"temperature\")",
          "queryType": "flux"
        },
        {
          "refId": "B",
          "query": "from(bucket: \"iot_sensors\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"environment\" and r._field == \"humidity\")",
          "queryType": "flux"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 30 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
